    "start:fr": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=fr --port=4201",
    "build:fr": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=fr",
    "start:bg": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=bg --port=4202",
    "build:bg": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=bg",
    "start:ru": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --configuration=ru --port=4203",
    "build:ru": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=ru",
    "test":     "ng test",
    "lint":     "ng lint",
    "e2e":      "ng e2e",
    "i18n:extract": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
